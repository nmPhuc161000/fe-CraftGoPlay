{
  "version": 2,
  "buildCommand": "git lfs install && git lfs fetch --all && git lfs checkout && npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/CraftGoPlayGame/Build/(.*)\\.wasm",
      "headers": [{ "key": "Content-Type", "value": "application/wasm" }]
    },
    {
      "source": "/CraftGoPlayGame/Build/(.*)\\.data",
      "headers": [{ "key": "Content-Type", "value": "application/octet-stream" }]
    },
    {
      "source": "/CraftGoPlayGame/Build/(.*)\\.js",
      "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
    }
  ],
  "rewrites": [
    { "source": "/CraftGoPlayGame/Build/(.*)", "destination": "/CraftGoPlayGame/Build/$1" },
    { "source": "/CraftGoPlayGame/TemplateData/(.*)", "destination": "/CraftGoPlayGame/TemplateData/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "regions": ["sin1"]

}
